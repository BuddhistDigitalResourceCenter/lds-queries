#QueryScope=Person
#QueryReturnType=Table
#QueryResults=A table containing the Id, primary_name, name_type and name_type value for people whose any name matches the last part of the url path
#QueryParams=URL,TYPE
#QueryUrl=/resource/person/klu sgrub


select distinct ?res ?Primary_Name
WHERE
{
?res a ?TYPE .
(?res ?sc ?Primary_Name) text:query ( skos:prefLabel ?URL ) .
}limit 100