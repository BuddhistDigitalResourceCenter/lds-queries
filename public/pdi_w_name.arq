#QueryScope=Work
#QueryReturnType=Table
#QueryResults=A table containing the Id and the name of the work whose name contains the NAME param value
#QueryParams=L_NAME
#QueryUrl=/lds-pdi/query?searchType=pdi_w_name&L_NAME=chos dbyings

select ?Work_ID ?Work_Name
where {
  ?Work_ID a :Work;
   skos:prefLabel ?Work_Name .
  Filter(contains(?Work_Name, ?L_NAME))
}