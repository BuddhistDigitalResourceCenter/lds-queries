#QueryScope=General
#QueryReturnType=Table
#QueryResults=A table containing the Id and matching literal for the given query and language tag with the given limit
#QueryParams=L_NAME,L_ALT1,L_ALT2
#QueryUrl=/lds-pdi/query?searchType=pdi_anyLabel&L_NAME=mkhan chen&L_ALT1='od zer&L_ALT2=ye shes

select distinct ?s ?lit
WHERE
{
  ?s ?sc ?lit .  
  Filter(?sc = skos:prefLabel || ?sc= rdfs:label)
  Filter(contains(?lit,?L_NAME) && (contains(?lit,?L_ALT1 ) || contains(?lit,?L_ALT2)))
} 