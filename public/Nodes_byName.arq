#QueryScope=General
#QueryReturnType=Table
#QueryResults=A table containing the Id and matching skos:prefLabel for the given query and language tag with the given limit
#QueryParams=L_NAME,LG_NAME,I_LIM
#QueryUrl=?searchType=Nodes_byName&L_NAME=("mkhan chen" AND ("'od zer" OR "ye shes"))&LG_NAME=bo-x-ewts&I_LIM=100


select distinct ?s ?lit
WHERE
{
  (?s ?sc ?lit) text:query ( ?L_NAME ) .
} limit ?I_LIM
