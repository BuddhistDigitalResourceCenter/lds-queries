#QueryScope=Work
#QueryReturnType=Table
#QueryResults=Returns the number of outline nodes for the given Work
#QueryParams=R_id
#QueryUrl=/Work_outline_count?R_id=bdr:W12827

#param.R_id.type=resource
#param.R_id.subtype=Work Resource ID
#param.R_id.desc=Work resource ID for the outline node count
#param.R_id.example=bdr:W12827

#output.?count.type=int
#output.?count.desc=the number of nodes in the outline

select (count(?part) as ?count)
where {
  ?R_id :workHasPart+ ?part .
}